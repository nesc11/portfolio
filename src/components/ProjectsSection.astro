---
import SectionLayout from "../layouts/SectionLayout.astro";
import ProjectsCommonContainer from "./ProjectsCommonContainer.astro";
const { pageTitle, customText } = Astro.props;
const projects = [
  {
    title: "Project 1",
    githubUrl: "#",
    liveUrl: "#",
    poster: {
      url: "https://images.pexels.com/photos/230544/pexels-photo-230544.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2",
      description: "Description for this project",
    },
  },
  {
    title: "Project 2",
    githubUrl: "#",
    liveUrl: "#",
    poster: {
      url: "https://images.pexels.com/photos/230544/pexels-photo-230544.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2",
      description: "Description for this project",
    },
  },
  {
    title: "Project 3",
    githubUrl: "#",
    liveUrl: "#",
    poster: {
      url: "https://images.pexels.com/photos/230544/pexels-photo-230544.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2",
      description: "Description for this project",
    },
  },
];
---

<SectionLayout sectionText="projects-section">
  <div>
    <p>{customText}</p>
    <h2>{pageTitle}</h2>
  </div>
  <div class="projects-container">
    {projects.map((project) => <ProjectsCommonContainer project={project} />)}
  </div>
</SectionLayout>
